{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "proxy": {
    "/auth/*": {
      "target": "http://localhost:5000"
    },
    "/connect/*": {
      "target": "http://localhost:5000"
    },
    "/api/*": {
      "target": "http://localhost:5000",
      "secure": false
    }
  },
  "dependencies": {
    "axios": "^0.18.0",
    "lodash": "^4.17.5",
    "moment": "^2.21.0",
    "moment-iterator": "^1.0.8",
    "react": "^16.2.0",
    "react-bootstrap": "^0.32.1",
    "react-date-range": "^0.9.4",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.4",
    "react-widgets": "^4.2.6",
    "react-widgets-moment": "^4.0.12",
    "recharts": "^1.0.0-beta.10",
    "redux": "^3.7.2",
    "redux-auth-wrapper": "^2.0.2",
    "redux-form": "^7.3.0",
    "redux-thunk": "^2.2.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-plugin-react": "^7.7.0",
    "redux-devtools-extension": "^2.13.2"
  }
}
